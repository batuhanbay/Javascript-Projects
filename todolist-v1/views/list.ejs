<%- include("header.ejs") -%> 
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>

    <div class="box">
      <% newListItems.forEach(newListItem => { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newListItem %></p>
      </div>
      <%}); %>

      <form action="/" method="POST" class="item">
        <input
          type="text"
          name="newItem"
          placeholder="Add Your New ToDo Item"
          autocomplete="off"
        />
        <button type="submit" name="button" value="<%= listTitle %>">+</button>
      </form>
    </div>
 <%- include("footer.ejs") %> 